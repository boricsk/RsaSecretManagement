<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <Title>RSA Secret Management</Title>
	  <PackageProjectUrl>https://github.com/boricsk/MifareClassic</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/boricsk/MifareClassic</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <Authors>Borics Krisztián</Authors>
